{
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "node",
		"allowJs": false,
		"strict": true,
		"jsx": "react-jsx",
		"sourceMap": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"lib": ["esnext", "dom"],
		"noImplicitAny": false
	},
	"include": [
		"src/**/*.ts",
		"src/**/*.d.ts",
		"src/**/*.tsx",
		"src/**/*.vue",
		"src/plugin/vite-plugin-closure-id.js",
		"src/plugin/test.ts"
	]
	// "include": ["src/**/*"]
}
