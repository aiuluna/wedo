{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "outDir": "dist/es",
    // "module": "CommonJS",
    // "outDir": "dist/commonjs",

    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "rootDir": "src",
    "allowJs": false,
    "downlevelIteration": true,
    "noImplicitAny": true,
    "strict": true,
    "moduleResolution": "node",
    "declaration": true,
  },
  "include": ["src/**/*"]
}
